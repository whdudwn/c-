#include<stdio.h>
typedef struct book {
	char writer[50];
	char name[50];
	int price;
	int isbn;

}; book;

void RaisebookPrice(book* something) {
	int p;
	printf("가격을 입력:");
	scanf("%d", &p);
	something->price += p;



}

